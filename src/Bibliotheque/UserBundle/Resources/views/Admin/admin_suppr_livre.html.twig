{% extends 'base_admin.html.twig' %}
{% block body %}
<h1>Supprimer un livre</h1>


{{ form_start(search) }}
<fieldset class="searchLivre">
			{{ form_label(search.recherche, "Rechercher un livre :") }}
			{{ form_widget(search.recherche) }}
			</fieldset>
{{ form_end(search) }}


<table>
	    {% for livres in livres %}
	        <tr>
	        	<td>{{ livres.titre }}</td>
	        </tr>
	    {% endfor %}
</table>

{% endblock %}